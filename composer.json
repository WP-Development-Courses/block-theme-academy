{
  "name": "block-theme-academy/project",
  "type": "project",
  "require": {
    "php": ">=7.4",
    "composer/installers": "^1.9",
    "wpackagist-plugin/create-block-theme": "2.6.0",
    "wpackagist-plugin/query-monitor": "3.16.4",
    "wpackagist-theme/twentytwentyone": "2.4",
    "wpackagist-theme/twentytwentytwo": "1.9",
    "wpackagist-theme/twentytwentythree": "1.6",
    "wpackagist-theme/twentytwentyfour": "1.3",
    "wpackagist-theme/twentytwentyfive": "1.0",
    "wpackagist-plugin/reorder-multisite-sites-dropdown": "^1.0"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "extra": {
    "installer-paths": {
      "plugins/{$name}/": ["type:wordpress-plugin"],
      "themes/{$name}/": ["type:wordpress-theme"]
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
